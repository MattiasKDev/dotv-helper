{
    "manifest_version": 3,
    "name": "DotV helper",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://play.dragonsofthevoid.com/*"
            ],
            "js": [
                "dataManager.js",
                "item-lookup.js"
            ]
        }
    ],
    "host_permissions": [
        "https://api.dragonsofthevoid.com/data/*"
    ],
    "permissions": [
        "storage"
    ]
}